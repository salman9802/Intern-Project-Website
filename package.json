{
  "name": "JunaidAlAtoor",
  "version": "1.0.0",
  "description": "",
  "main": "app.mjs",
  "scripts": {
    "start": "node app",
    "dev": "nodemon . --delay 1",
    "tailwindcss": "tailwindcss",
    "build:css": "tailwindcss -i ./public/css/input.css -o ./public/css/style.css",
    "watch:css": "tailwindcss -i ./public/css/input.css -o ./public/css/style.css --watch",
    "dev2": "start npm run watch:css && start npm run dev",
    "dev3": "npm-run-all --parallel watch:css dev",
    "dev4": "run-p watch:css dev",
    "dev5": "npm-run-all --parallel dev watch:css",
    "dev6": "concurrently \"npm run watch:css\" \"npm run dev\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.2.2",
    "got": "^14.2.1",
    "nodemon": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "mongoose": "^8.1.2"
  },
  "type": "module",
  "nodemonConfig": {
    "ext": "js,ejs,html,mjs"
  }
}
